{
    "name": "icp/wp-redis-plugin",
    "description": "A persistent cache backend for WordPress powered by Redis.",
    "keywords": [
        "wordpress-dropin",
        "wordpress-cache",
        "cache-object",
        "cache-page",
        "cache-advanced",
        "redis",
        "wordpress"
    ],
    "homepage": "http://ph-prbit-02.shp.cloud:7990/scm/pw/wp-redis-plugin.git",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Jose Linares"
        }
    ],
    "require": {
        "php": ">=7.1",
        "composer/installers": "^1.0",
        "koodimonni/composer-dropin-installer": "^1.2"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.3"
    },
    "suggest": {
        "ext-redis": "Using a native-Redis interface",
        "predis/predis": "Using a PHP-Redis interface"
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "scripts": {
        "lint": [
            "phpcs"
        ]
    },
    "support": {
        "issues": "http://ph-prbit-02.shp.cloud:7990/scm/pw/wp-redis-plugin.git"
    }
}
